{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "main": "dist/app.js",
  "scripts": {
    "start": "cross-env PAYLOAD_CONFIG_PATH=dist/payload.config.js NODE_ENV=production node --env-file=.env ./dist/app.js",
    "build": "tsc && npm run copyfiles",
    "build:prod": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload build && tsc && npm run copyfiles",
    "dev": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts nodemon --env-file=.env",
    "types": "payload generate:types",
    "copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png,js}\" dist/",
    "test": "mocha test/**/*.spec.ts"
  },
